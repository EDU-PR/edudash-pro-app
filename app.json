{
  "expo": {
    "name": "EduDashPro",
    "slug": "edudashpro",
    "owner": "k1ng-devops",
    "version": "1.0.3",
    "runtimeVersion": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "edudashpro",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*",
      "locales/**/*.json"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0f"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.k1ngdevops.edudashpro",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs access to your microphone so Dash can hear your questions and provide voice responses.",
        "NSCameraUsageDescription": "This app needs camera access so you can take photos to share with Dash.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library so you can share images with Dash.",
        "UIBackgroundModes": [
          "audio",
          "voip"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.edudashpro.app",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "blockedPermissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "versionCode": 4,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "www.edudashpro.org.za",
              "pathPrefix": "/landing"
            },
            {
              "scheme": "https",
              "host": "edudashpro.org.za",
              "pathPrefix": "/landing"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-router",
      "expo-updates",
      "sentry-expo",
      "expo-audio",
      "@react-native-voice/voice",
      "expo-speech-recognition",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2808416461095370~5255516826",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ],
      "expo-localization",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#00f5ff",
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/notification.wav"
          ]
        }
      ],
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "EduDash Pro needs camera access for video calls with teachers and parents",
          "microphonePermission": "EduDash Pro needs microphone access for voice and video calls"
        }
      ],
      "./plugins/withAndroidManifestFix.js"
    ],
    "experiments": {
      "typedRoutes": false
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "EduDash Pro - AI-Powered Educational Platform",
      "shortName": "EduDash Pro",
      "lang": "en",
      "scope": "/",
      "themeColor": "#00f5ff",
      "backgroundColor": "#0a0a0f",
      "description": "Revolutionary AI-powered educational platform for preschools. Trusted by educators worldwide for next-generation learning experiences with Society 5.0 technology.",
      "keywords": [
        "education",
        "preschool",
        "AI",
        "learning",
        "teachers",
        "parents",
        "lessons",
        "artificial intelligence",
        "educational technology",
        "edtech"
      ],
      "author": "EduDash Pro Team",
      "viewport": "width=device-width, initial-scale=1, shrink-to-fit=no",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "any",
      "bundler": "metro",
      "output": "single",
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons"
          ]
        }
      },
      "serviceWorker": {
        "enabled": true
      }
    },
    "extra": {
