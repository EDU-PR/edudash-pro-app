{
  "name": "edudash-pro",
  "version": "1.0.2",
  "main": "index.js",
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-chart-kit",
          "@picovoice/porcupine-react-native",
          "dotenv",
          "exceljs",
          "i18next-browser-languagedetector",
          "js-yaml",
          "sentry-expo",
          "supabase",
          "tslib",
          "react-native-webrtc",
          "@daily-co/react-native-daily-js",
          "@config-plugins/react-native-webrtc",
          "react-native-pdf",
          "react-native-blob-util",
          "expo-av",
          "react-native-background-timer",
          "react-native-incall-manager"
        ],
        "listUnknownPackages": false
      }
    },
    "install": {
      "exclude": [
        "react",
        "react-dom",
        "react-native",
        "@react-native-community/datetimepicker",
        "@types/react",
        "expo-auth-session"
      ]
    }
  },
  "scripts": {
    "postinstall": "patch-package",
    "docs:policy:check": "node ./scripts/docs-policy-check.js",
    "eas:project": "./scripts/switch-eas-project.sh",
    "eas:old": "./scripts/switch-eas-project.sh old",
    "eas:new": "./scripts/switch-eas-project.sh new",
    "ota:mark1": "./scripts/switch-eas-project.sh old && eas update --channel production",
    "ota:release8": "./scripts/switch-eas-project.sh new && eas update --channel production",
    "start": "npx expo start --dev-client --host localhost",
    "start:clear": "npx expo start --dev-client --host localhost --clear",
    "open:android": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:8083 tcp:8083 && adb shell am start -n com.edudashpro/expo.modules.devlauncher.launcher.DevLauncherActivity -a android.intent.action.VIEW -d \"exp://expo-development-client/?url=http://127.0.0.1:8081\"",
    "dev:android": "npm run start:clear & sleep 5 && npm run open:android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:dev": "cross-env WEB_PORT=8082 expo start --web --port 8082",
    "web:clear": "node -e \"['8081','19000','19001','19006','8082'].forEach(p=>{try{require('child_process').execSync('fuser -k '+p+'/tcp',{stdio:'ignore'})}catch(e){}});\" && rm -rf .expo .cache && find /tmp -maxdepth 1 -type d -name 'metro-*' -exec rm -rf {} + 2>/dev/null || true",
    "web:build": "expo export --platform web --output-dir dist --clear",
    "vercel-build": "npm run web:build && bash scripts/copy-locales.sh && node scripts/patch-manifest-link.js",
    "web:preview": "serve -s dist -l 4173",
    "web:prod": "expo start --web --no-dev --minify",
    "cache:clear": "bash scripts/clear-caches.sh",
    "ota:preview:android": "npm run cache:clear && cross-env NODE_OPTIONS=--max-old-space-size=8192 eas update --channel preview --platform android --clear-cache --message \"Preview: PayFast return deep link, tier refresh, Dash AI nav hide, promo pricing\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 200",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check:file-sizes": "node scripts/check-file-sizes.mjs",
    "check:backups": "bash scripts/scan-backups.sh",
    "check:todos": "bash scripts/scan-todos.sh",
    "check:console": "rg -n --hidden --glob '!node_modules/**' 'console\\.log\\('",
    "inspect-db": "tsx scripts/inspect-remote-db.ts",
    "inspect-db-full": "tsx scripts/inspect-with-service-role.ts",
    "setup-rls": "tsx scripts/apply-rls-setup.ts",
    "reset-migrations": "tsx scripts/reset-migration-history.ts",
    "lint:sql": "./lint-sql.sh lint",
    "fix:sql": "./lint-sql.sh fix",
    "sql:rules": "./lint-sql.sh rules",
    "sql:config": "./lint-sql.sh config",
    "fix-auth": "node scripts/fix-auth.js",
    "i18n:export": "node scripts/export-for-translation.js",
    "i18n:verify": "node scripts/verify-translations.js",
    "build:android:apk": "npx eas build --platform android --profile production-apk --local",
    "build:android:aab": "node scripts/bump-android-version.mjs && npx eas build --platform android --profile production --clear-cache --non-interactive",
    "build:android:preview": "npx eas build --platform android --profile preview --local",
    "eas-build-pre-install": "echo 'EAS Build: Pre-install hook running'",
    "eas-build-post-install": "echo 'Typecheck temporarily disabled for build - will fix TS errors in separate PR'",
    "eas-build-on-success": "echo 'EAS Build: Build completed successfully'",
    "eas-build-on-error": "echo 'EAS Build: Build failed'",
    "eas-build-on-complete": "echo 'EAS Build: Process completed'"
  },
